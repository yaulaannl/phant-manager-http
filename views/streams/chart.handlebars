  <div class="row stream-metadata">

    <div class="col-md-12">

      <div class="row">

	<!-- title -->
        <div class="title">
          <h1>
            {{stream.title}}
            <span class="small">{{stream.description}}</span>
          </h1>
        </div>

     </div>
   </div> <!-- col -->
